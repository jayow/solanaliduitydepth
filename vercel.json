{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_TLS_REJECT_UNAUTHORIZED": "0"
  },
  "crons": [
    {
      "path": "/api/liquidity-calculate",
      "schedule": "0 * * * *"
    }
  ]
}

